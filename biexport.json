{
    "name": "biExport",
    "description": "biExport 4 SAP SAC PDF/PPT/DOC/XLS",
    "eula": "",
    "vendor": "bi excellence Software GmbH",
    "license": "",
    "id": "com.biexcellence.openbi.sap.sac.export",
    "newInstancePrefix": "biExport",
    "version": "1.0.0",
    "icon": "data:image/png;base64,",
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-biexcellence-openbi-sap-sac-export",
            "url": "https://raw.githubusercontent.com/biexcellence/biexport-sac/master/biexport.js",
            "integrity": "",
            "ignoreIntegrity" : true
        },
        {
            "kind": "styling",
            "tag": "com-biexcellence-openbi-sap-sac-export-aps",
            "url": "https://raw.githubusercontent.com/biexcellence/biexport-sac/master/aps_biexport.js",
            "integrity": "",
            "ignoreIntegrity" : true
        }
    ],
    "properties": {
        "serverURL": {
            "type": "string",
            "description": "The URL to the open bi Repository installation that handles the export. If you use an installation on your local machine, change this property to http://localhost:9091, otherwise to the address and port of your installation.",
            "default": "https://designstudioexport.biexcellence.com"
        },
        "filename": {
            "type": "string",
            "description": "The file name of the export document. The extension will be determined automatically. You can use placeholders (manual section 3.9).",
            "default": 0
        },
        "pdfButton": {
            "type": "string",
            "description": "The text that is displayed on the PDF Export Button. Empty the property to hide the button.",
            "default": "PDF Export"
        },
        "pptButton": {
            "type": "string",
            "description": "The text that is displayed on the PPT Export Button. Empty the property to hide the button.",
            "default": "PPT Export"
        },
        "docButton": {
            "type": "string",
            "description": "The text that is displayed on the DOC Export Button. Empty the property to hide the button.",
            "default": "DOC Export"
        },
        "xlsButton": {
            "type": "string",
            "description": "The text that is displayed on the XLS Export Button. Empty the property to hide the button.",
            "default": "XLS Export"
        },
        "width": {
            "type": "number",
            "default": 200
        },
        "height": {
            "type": "number",
            "default": 100
        }
    },
    "methods": {
        "doExport": {
            "description": "Get the Server URL",
            "parameters": [
                {
                    "name": "format",
                    "type": "string",
                    "description": "The export format: \"PDF\", \"PPTX\", \"DOCX\", \"XLSX\""
                }
            ],
            "body": "this.startExport(format);"
        },
        "getServerUrl": {
            "returnType": "string",
            "description": "Get the Server URL",
            "parameters": [],
            "body": "return this.serverURL;"
        },
        "setServerUrl": {
            "description": "Set the Server URL",
            "parameters": [
                {
                    "name": "value",
                    "type": "string",
                    "description": "The new Server URL"
                }
            ],
            "body": "this.serverURL = value;"
        },
        "getFilename": {
            "returnType": "string",
            "description": "Get the filename",
            "parameters": [],
            "body": "return this.filename;"
        },
        "setFilename": {
            "description": "Set the filename",
            "parameters": [
                {
                    "name": "value",
                    "type": "string",
                    "description": "The new filename"
                }
            ],
            "body": "this.filename = value;"
        },
        "getPdfButton": {
            "returnType": "string",
            "description": "Get the PDF button text",
            "parameters": [],
            "body": "return this.pdfButton;"
        },
        "setPdfButton": {
            "description": "Set the PDF button text",
            "parameters": [
                {
                    "name": "value",
                    "type": "string",
                    "description": "The new PDF button text"
                }
            ],
            "body": "this.pdfButton = value;"
        },
        "getPptButton": {
            "returnType": "string",
            "description": "Get the PPT button text",
            "parameters": [],
            "body": "return this.pptButton;"
        },
        "setPptButton": {
            "description": "Set the PPT button text",
            "parameters": [
                {
                    "name": "value",
                    "type": "string",
                    "description": "The new PPT button text"
                }
            ],
            "body": "this.pptButton = value;"
        },
        "getDocButton": {
            "returnType": "string",
            "description": "Get the DOC button text",
            "parameters": [],
            "body": "return this.docButton;"
        },
        "setDocButton": {
            "description": "Set the DOC button text",
            "parameters": [
                {
                    "name": "value",
                    "type": "string",
                    "description": "The new DOC button text"
                }
            ],
            "body": "this.docButton = value;"
        },
        "getXlsButton": {
            "returnType": "string",
            "description": "Get the XLS button text",
            "parameters": [],
            "body": "return this.xlsButton;"
        },
        "setXlsButton": {
            "description": "Set the XLS button text",
            "parameters": [
                {
                    "name": "value",
                    "type": "string",
                    "description": "The new XLS button text"
                }
            ],
            "body": "this.xlsButton = value;"
        }
    },
    "events": {
        "onStartExport": {
            "description": "Specify the SAC Script that shall be triggered when the Export process is started. If you set the publishing property Parse CSS to true, all image resources are collected in this phase."
        },
        "onSendExport": {
            "description": "Specify the SAC Script that shall be triggered when the Export process sends the content to the Export service."
        },
        "onReturnExport": {
            "description": "Specify the SAC Script that shall be triggered when the Export process receives the generated document from the Export service."
        },
        "onErrorExport": {
            "description": "Specify the SAC Script that shall be triggered when an error is reported by the Export process."
        }
    }
}